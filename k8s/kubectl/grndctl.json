{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "grndctl",
    "namespace": "default",
    "labels": {
      "app.kubernetes.io/instance": "grndctl",
      "app.kubernetes.io/name": "grndctl"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "http",
        "protocol": "TCP",
        "port": 8080,
        "targetPort": "http"
      }
    ],
    "selector": {
      "app.kubernetes.io/instance": "grndctl",
      "app.kubernetes.io/name": "grndctl"
    },
    "type": "ClusterIP",
    "sessionAffinity": "None"
  },
  "status": {
    "loadBalancer": {}
  }
}
{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "grndctl",
    "namespace": "default",
    "labels": {
      "app.kubernetes.io/instance": "grndctl",
      "app.kubernetes.io/name": "grndctl"
    }
  },
  "spec": {
    "replicas": 3,
    "selector": {
      "matchLabels": {
        "app.kubernetes.io/instance": "grndctl",
        "app.kubernetes.io/name": "grndctl"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app.kubernetes.io/instance": "grndctl",
          "app.kubernetes.io/name": "grndctl"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "grndctl",
            "image": "michaelvdisalvo/grndctl-akka:1.0.0-SNAPSHOT",
            "ports": [
              {
                "name": "http",
                "containerPort": 8080,
                "protocol": "TCP"
              }
            ],
            "resources": {},
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "imagePullPolicy": "Never"
          }
        ],
        "restartPolicy": "Always",
        "terminationGracePeriodSeconds": 30,
        "dnsPolicy": "ClusterFirst",
        "securityContext": {},
        "schedulerName": "default-scheduler"
      }
    },
    "strategy": {
      "type": "RollingUpdate",
      "rollingUpdate": {
        "maxUnavailable": "25%",
        "maxSurge": "25%"
      }
    },
    "revisionHistoryLimit": 10,
    "progressDeadlineSeconds": 600
  }
}